        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "uglify-es": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "bootstrap-vue": "0.20.1",
        "chart.js": "2.6.0",
        "font-awesome": "^4.7.0",
        "simple-line-icons": "^2.4.1",
